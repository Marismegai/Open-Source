function Tween(Pos)
    Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    end
    pcall(function()
        fly = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/300, Enum.EasingStyle.Linear), {CFrame = Pos})
    end)
    fly:Play()
    if Distance <= 300 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos2
    end
    if StopTween == true then
        fly:Cancel()
        NoClip = false
    end
end

function Teleport(P)
    Distance = (P.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    end
    pcall(function()
        fly1 = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear), {CFrame = P})
    end)
    fly1:Play()
    if Distance >= 1 then
        Speed = 300
    end
    if StopTween == true then
        fly1:Cancel()
        NoClip = false
    end
    NoClip = true
    wait(Distance/Speed)
    NoClip = false
end

Tween(CFrame.new(999, 9999999, 999))
print("Smooth")
