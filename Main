if game.PlaceId == 2753915549 then
    FirstWorld = true
elseif game.PlaceId == 4442272183 then
    SecondsWorld = true
elseif game.PlaceId == 7449423635 then
    ThirdWorld = true
else
    game:GetService("Players").LocalPlayer:Kick("You Are Blacklist ' How Do Run! ' ")
end

